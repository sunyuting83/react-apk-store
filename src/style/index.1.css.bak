@import url('https://cdn.bootcss.com/font-awesome/4.7.0/css/font-awesome.min.css');
a,
input {
    outline: 0
}

a,
a:hover,
body {
    color: #333
}

.clear,
.clearfix:after {
    clear: both
}

@media only screen and (max-width:27.62rem) {
    html {
        font-size: 1rem
    }
}

@media only screen and (min-width:27.69rem) and (max-width:28.77rem) {
    html {
        font-size: 1.15rem
    }
}

@media only screen and (min-width:28.85rem) and (max-width:30.69rem) {
    html {
        font-size: 1.15rem
    }
}

@media only screen and (min-width:30.77rem) and (max-width:36.85rem) {
    html {
        font-size: 1.23rem
    }
}

@media only screen and (min-width:36.92rem) and (max-width:41.46rem) {
    html {
        font-size: 1.54rem
    }
}

@media only screen and (min-width:41.54rem) and (max-width:49.15rem) {
    html {
        font-size: 1.69rem
    }
}

@media only screen and (min-width:49.23rem) and (max-width:55.31rem) {
    html {
        font-size: 2rem
    }
}

@media only screen and (min-width:55.38rem) {
    html {
        font-size: 2.31rem
    }
}

@media only screen and (min-width:64.5rem) {
    html {
        font-size: 1.25rem
    }
}

html {
    overflow-y: scroll
}

body {
    margin: 0 auto;
    background-color: #f1f1f1;
    font: .6rem/1.5 Microsoft Yahei, sans-serif;
    text-align: center;
    text-size-adjust: 100%;
}

dd,
div,
dl,
dt,
form,
h1,
h2,
h3,
h4,
h5,
h6,
img,
input,
li,
ol,
p,
select,
span,
table,
td,
th,
ul {
    margin: 0;
    padding: 0;
    font-weight: 400
}

iframe,
img {
    border: none
}

li,
ol,
ul {
    list-style: none
}

table {
    border-collapse: collapse;
    border-spacing: 0
}

input {
    font-family: Microsoft Yahei, sans-serif
}

b,
cite,
em,
font,
i,
strong,
sub,
sup {
    font-weight: 400;
    font-style: normal;
    font-size: 100%
}

a {
    text-decoration: none;
    tap-highlight-color: rgba(0, 0, 0, 0);
}

.clearfix {
    zoom: 1
}

body,
html {
    height: 100%;
    width: 100%
}

input::-ms-clear {
    display: none
}

button,
input[type=submit],
input[type=reset],
input[type=button],
input[type=search] {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none
}

.h1,
.h3,
.h5 {
    font-weight: inherit;
}

.img-100 {
    width: 100%;
    display: block
}

/*中间件*/

.content {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    width: 100%;
    overflow-y: scroll;
    overflow-x: hidden;
    -webkit-overflow-scrolling: touch;
    background-color: #f1f1f1;
}

.content>* {
    transform: translateZ(0);
}

.bar-tab {
    position: fixed;
    right: 0;
    left: 0;
    z-index: 10;
    bottom: 0;
    height: 2.85rem;
    padding: 0;
    border-top: 1px solid #e6e6e6;
    border-bottom: 0;
    text-align: center;
    background: #fafafa;
    align-items: center
}

.bar-tab i {
    font-size: 1.26rem;
    margin-top: .25rem;
    color: #bbb
}

.bar-tab .col.active i {
    color: #333
}

.bar-tab span {
    display: block;
    color: #bbb
}

.bar-tab .col.active span {
    color: #333
}

/*顶部*/

header~.content {
    padding-top: 2.5rem;
    padding-bottom: 2.85rem;
}

header.packup~.content {
    padding-top: 2.5rem;
    padding-bottom: 2.85rem;
}

header~.content.nobottom {
    padding-top: 2.5rem;
    padding-bottom: 0;
}

header {
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    height: 2.5rem;
    background: #FFF;
    z-index: 99;
    display: -webkit-flex;
    /* Safari */
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: space-between;
    align-items: center;
    padding: 0;
    box-sizing: border-box;
}

header.packup {
    /* -ms-transform: translate3d(0, -2.5rem, 0);
    -moz-transform: translate3d(0, -2.5rem, 0);
    -webkit-transform: translate3d(0, -2.5rem, 0);
    -o-transform: translate3d(0, -2.5rem, 0);
    transform: translate3d(0, -2.5rem, 0); */
}

/*首页分类*/

.class {
    display: none;
    z-index: 9;
    background: #fff;
}

.class .col>span {
    display: block;
    line-height: 1.07rem;
    font-size: 1rem;
    margin-top: 0.30rem;
}

/*栅格*/

.row {
    display: flex;
    width: 100%;
    box-sizing: border-box;
    align-items: center
}

.row-wrap {
    flex-wrap: wrap;
}

.row+.row {
    margin-top: -1px;
}

.row.align-items {
    align-items: center
}

.col {
    flex: 1;
    display: block;
    padding: 0.25rem;
    width: 100%;
    box-sizing: border-box;
    position: relative;
}

.col-offset-10 {
    margin-left: 10%
}

.col-offset-5 {
    margin-left: 5%
}

.col-offsetr-5 {
    margin-right: 5%
}

.col-offset-20 {
    margin-left: 20%
}

.col-offset-25 {
    margin-left: 25%
}

.col-offset-33,
.col-offset-34 {
    margin-left: 33.3333%
}

.col-offset-50 {
    margin-left: 50%
}

.col-offset-66,
.col-offset-67 {
    margin-left: 66.6666%
}

.col-offset-75 {
    margin-left: 75%
}

.col-offset-80 {
    margin-left: 80%
}

.col-offset-90 {
    margin-left: 90%
}

.col-10 {
    flex: 0 0 10%;
    max-width: 10%
}

.col-15 {
    flex: 0 0 15%;
    max-width: 15%
}

.col-20 {
    flex: 0 0 20%;
    max-width: 20%
}

.col-25 {
    flex: 0 0 25%;
    max-width: 25%
}

.col-33,
.col-34 {
    flex: 0 0 33.3333%;
    max-width: 33.3333%
}

.col-32 {
    flex: 0 0 32.9999%;
    max-width: 32.9999%
}

.col-50 {
    flex: 0 0 50%;
    max-width: 50%
}

.col-65 {
    flex: 0 0 65.6666%;
    max-width: 65.6666%
}

.col-66,
.col-67 {
    flex: 0 0 66.6666%;
    max-width: 66.6666%
}

.col-75 {
    flex: 0 0 75%;
    max-width: 75%
}

.col-80 {
    flex: 0 0 80%;
    max-width: 80%
}

.col-90 {
    flex: 0 0 90%;
    max-width: 90%
}

.row-no-padding,
.row-no-padding>.col {
    padding: 0
}

[name="PlayList"] .col {
    min-width: 15%;
    overflow: hidden;
}

[name="PlayList"] .d-col {
    min-width: 15%;
    margin-right: 3%;
}

/*block*/

.block {
    background: #fff;
    margin-bottom: 0.375rem;
}

.last-block {
    background: #fff;
    margin-top: -0.4rem;
    padding: 0.625rem
}

.bt-block>div {
    display: inline-block;
    width: 50%;
    box-sizing: border-box;
    margin-top: 0.3125rem;
    background: #fff;
    vertical-align: top;
    border-top: 0.3125rem solid #f1f1f1;
    height: 15rem
}

.bt-block>div:first-child {
    border-right: 0.3125rem solid #f1f1f1
}

.bt-block h1 {
    font-size: 0.8125rem;
    text-align: left;
    padding-left: 0.9375rem;
    color: #a5a5a5;
    margin-top: 0.625rem
}

.bt-block .tagls {
    text-align: left;
    padding-left: 0.625rem;
}

.bt-block .tagls>span {
    display: inline-block;
    padding: 0.3125rem 0.625rem;
    margin: 0.4375rem 0.25rem !important;
    max-width: 6rem;
    font-size: 0.75rem;
    color: #ff674b;
    background-color: transparent;
    white-space: nowrap;
    cursor: pointer;
    text-align: center;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border-radius: 3.125rem;
    position: relative;
}

.bt-block .tagls>span:before {
    position: absolute;
    top: 0;
    left: 0;
    content: "";
    z-index: 2;
    pointer-events: none;
    display: block;
    width: 200%;
    height: 200%;
    -moz-border-radius: 3.125rem;
    border-radius: 3.125rem;
    border: 1px solid;
    -webkit-transform: scale(.5);
    -moz-transform: scale(.5);
    -ms-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    -moz-transform-origin: 0 0;
    -ms-transform-origin: 0 0;
    transform-origin: 0 0;
    color: #e3e3e3;
}

/*block-border*/

.block-border {
    border-top: 1px solid #ddd;
    border-bottom: 1px solid #ddd;
}

.block-border:first-child {
    margin-top: 0px;
}

.block.block-no-margin {
    margin-bottom: 0
}

div.inner {
    background: #fff;
    height: 49.97% !important
}

div.inner,
div.inner ul.list {
    display: flex;
    flex-flow: row nowrap;
}

div.inner ul.list {
    flex-shrink: 0;
    padding: 0 .4rem;
    background: #fff
}

div.inner ul.list li {
    flex-shrink: 0;
    position: relative;
}

div.inner ul.list li a,
div.inner ul.list li .item-infoImg {
    display: inline-block;
    padding: 0.5rem 0.9375rem;
    flex-grow: 2;
    flex-basis: 0;
    font-size: 1.07rem;
    position: relative;
    width: 4.6rem
}

div.inner ul.list li .item-infoImg {
    width: 7.1875rem;
    padding: 0.5rem 0.4rem;
}

div.inner .apkmore {
    text-align: center
}

div.inner .apkmore h1 {
    font-size: .9rem;
    display: block;
    width: 100%;
    overflow: hidden;
    word-break: keep-all;
    white-space: nowrap;
    text-align: center;
    text-overflow: ellipsis;
}

div.inner .apkmore h5 {
    font-size: 0.75rem;
    color: #999;
    margin-bottom: 0.1875rem;
}

div.inner .download {
    width: 60%;
}

/* item-swiper */

.item-swiper {
    position: fixed;
    left: 0;
    top: 0;
    background: #000;
    z-index: -1;
    opacity: 0;
    transition: all .25s;
}

.item-swiper.active {
    width: 100%;
    height: 100%;
    z-index: 999;
    opacity: 1;
}

.item-swiper .swiper-pagination {
    width: 100%;
    bottom: 10px;
}

.item-swiper .swiper-pagination .swiper-pagination-bullet {
    width: 0.5rem;
    height: 0.5rem;
    border-radius: 100%;
    background: #FFF;
    opacity: .7;
    margin-right: 0.5rem
}

.item-swiper .swiper-pagination .swiper-pagination-bullet-active {
    opacity: 1;
    background: #007aff;
}

/* item-header */

.item-header {
    display: flex;
    width: 100%;
    height: 2.5rem;
    align-items: center
}

.item-header .back,
.item-header .search,
.item-header .search-back,
.item-header .search-btn {
    width: 3.4375rem;
    height: 1.375rem;
}

.item-header .search-back,
.item-header .search-btn {
    background: #fbfbfb;
    border-radius: 3px;
    line-height: 1.75rem;
    margin-right: 0.5rem;
    height: 1.75rem;
    font-size: 0.875rem
}

.item-header .search-btn {
    background: #1bb6fe;
    color: #fff;
    font-size: 0.8125rem;
    border: none;
}

.item-header .item-nav {
    flex: 1;
    font-size: 0.9375rem;
    font-weight: bold
}

.item-header i {
    font-size: 1.125rem
}

.item-header i.fa-angle-left {
    font-size: 1.6rem;
    margin-top: -0.1875rem;
}

.item-header .logo {
    flex: 0 0 26%;
    box-sizing: border-box;
    max-width: 26%
}

.item-header .logo img {
    height: 1.25rem;
    margin-top: 0.375rem
}

.item-header .searchInput {
    flex: 0 0 74%;
    max-width: 74%;
    position: relative;
}

.item-header .searchI {
    position: relative;
    height: 1.75rem;
    color: #d9d9d9;
    background-color: #f3f3f3;
    border-radius: 3px;
    align-items: center;
    padding-top: 0.3rem;
    padding-left: 0.5rem;
    width: 96%;
    text-align: left;
    box-sizing: border-box;
    font-size: 0.8125rem;
    overflow: hidden;
}

.item-header .searchI>span {
    color: #999
}

.item-header .searchI:before {
    position: absolute;
    top: 0;
    left: 0;
    content: "";
    z-index: 2;
    pointer-events: none;
    display: block;
    width: 200%;
    height: 200%;
    -moz-border-radius: 50px;
    border-radius: 50px;
    border: 1px solid;
    -webkit-transform: scale(.5);
    -moz-transform: scale(.5);
    -ms-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    -moz-transform-origin: 0 0;
    -ms-transform-origin: 0 0;
    transform-origin: 0 0;
    -moz-border-radius: 9px;
    border-radius: 9px;
}

.item-header .searchI i.fa-search {
    font-size: 1rem;
    margin-right: 0.5rem;
    color: #999
}

.item-header .searchI i.fa-times-circle {
    position: absolute;
    right: 0px;
    font-size: 1rem;
    margin-right: 0.5rem;
    color: #999;
    z-index: 99
}

.item-header .searchI>input {
    position: absolute;
    top: 0;
    left: 0;
    border: none;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    background-color: transparent;
    padding-left: 1.875rem;
    font-size: 0.875rem;
    color: #666
}

/* 显示或隐藏联想列表 */

.item-header .search-over {
    position: absolute;
    width: 100%;
    text-align: left;
    top: 2.5rem;
    padding-top: 0.3125rem;
    background: #fff;
    z-index: 99;
    opacity: 1;
}

.item-header .search-over.hd {
    z-index: -99;
    opacity: 0;
}

.item-header .search-over .list {
    display: block;
    height: 2.8125rem;
    line-height: 2.8125rem;
    padding: 0 1.25rem;
    background: linear-gradient(180deg, #e1e1e1, transparent 67%, transparent 0) no-repeat bottom;
    -moz-background-size: 100% 1px;
    background-size: 100% 1px;
    background-position: 0 0;
}

.item-header .search-over>.list>i {
    color: #d9d9d9;
    margin-right: 0.5rem;
    font-size: 0.625rem
}

.item-header .search-over>.list>span {
    color: #333;
    font-size: 0.875rem
}

.item-header .search-over>a>div.first {
    padding-top: 0.5rem;
    padding-bottom: 0.5rem
}

.item-header .search-over>a>div.first>div {
    display: inline-block;
    vertical-align: top
}

.item-header .search-over>a>div.first img {
    width: 2.625rem;
    height: 2.625rem
}

.item-header .search-over>a>div.first>div.first-img {
    margin-left: 1.25rem;
    margin-right: 0.9375rem
}

.item-header .search-over>a>div.first>div.first-text h1 {
    font-size: 1rem;
    font-weight: blod
}

.item-header .search-over>a>div.first>div.first-text h5 {
    font-size: 0.75rem;
    color: #999
}

/* item */

.item {
    display: inline-block;
    background: #fff
}

.item .item-img {
    margin-top: 4.6875rem;
}

.item img {
    width: 4.6875rem;
    height: 4.6875rem;
}

.item h1 {
    font-size: 1.2rem;
    font-weight: bold;
}

.item h5 {
    margin: 0.1875rem 0;
    color: #999;
    font-size: 0.75rem
}

.item h5 span {
    display: inline-block;
    margin-right: 0.9375rem
}

.item-dl {
    padding: 1.2rem 0;
    background: #fff
}

.item-download {
    margin: 0 auto;
    display: block;
    width: 18.0625rem;
    line-height: 1.125rem;
    height: 2.3125rem;
    font-size: 0.9375rem;
    color: #fff;
    background-image: linear-gradient(35deg, #25cdff, #467bf3);
    background-color: #467bf3;
    white-space: nowrap;
    cursor: pointer;
    text-align: center;
    box-sizing: border-box;
    padding: 0 0.8438rem;
    border-radius: 3.125rem;
    position: relative;
    padding-top: 0.4375rem;
    padding-bottom: 0.5625rem;
    border: none
}

.item-download i,
.item-download div {
    display: inline-block;
    margin-top: 0.28rem;
    vertical-align: middle
}

.item-download div {
    width: 85%;
    border-left: 1px solid #fff;
    margin-left: 0.5rem;
}

.item-download i {
    font-size: 1.125rem
}

div.inner ul.list li .item-infoImg img {
    border-radius: 0.625rem;
}

.anniu {
    padding: 0.9375rem
}

.anniu i.fa {
    font-size: 1.25rem;
    margin-left: 0.5rem;
    vertical-align: middle
}

div.inner ul.list li .tag {
    width: auto;
    padding: 0.2rem 0.7rem;
    margin-right: 0.3125rem;
    font-size: 0.75rem;
    color: #ebb356;
    background-color: #fff8e5;
    border-radius: 3.125rem;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.bootom-down {
    box-shadow: 0 0 10px #999;
    position: fixed;
    width: 100%;
    bottom: 0;
    left: 0;
    z-index: -1;
    opacity: 0;
    height: 3.625rem;
    background: #fff;
    padding: 0.8125rem 1.125rem;
    transition: all .15s;
    text-align: left
}

.bootom-down.active {
    z-index: 99;
    opacity: 1;
}

.bootom-down img {
    width: 2rem
}

.bootom-down div {
    padding: 0
}

.bootom-down div>h1 {
    font-size: 0.875rem;
    font-weight: bold
}

.bootom-down div>span {
    font-size: 0.6875rem;
    color: #999
}

.bootom-down .bt-down {
    color: #fff;
    background-image: linear-gradient(35deg, #25cdff, #467bf3);
    background-color: #467bf3;
    border: none;
    font-size: 0.8125rem;
    box-sizing: border-box;
    padding: 0.125rem 0.8438rem 0.1875rem;
    border-radius: 3.125rem;
}

/*catalog*/

.catalog,
.players {
    background: #fff;
    padding: 0 0.38rem;
    margin-top: -1px;
    position: relative;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -ms-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
    color: #3a3a3a;
}

.players {
    padding: 0;
}

.catalog.no-border {
    border-top: none;
    border-bottom: none
}

.catalog.margin-bottom {
    margin-bottom: .8rem;
}

.catalog img {
    max-width: 100%;
    display: block;
    margin: 0 auto;
}

.catalog i.icon {
    width: 0.65625rem;
    height: 0.65625rem;
    position: absolute;
}

.catalog i.icon-arrow-right {
    background-position: -2.375rem -0.03125rem;
    right: .4rem;
    top: 50%;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%);
}

.catalog.active i.icon-arrow-right {
    background-position: -2.4375rem -0.875rem;
}

.catalog-divider {
    background: #fefefe;
    border-bottom: 1px solid #eee;
    font-size: 0;
    display: none;
}

.catalog-divider.active {
    display: block;
}

.b-title {
    font-size: 1.07rem;
    text-align: left;
    font-weight: bold;
    flex: 0 0 93%;
    padding-left: 0.6875rem
}

.title-more {
    flex: 0 0 7%;
    font-size: 1.4rem
}

/*电影列表*/

.apkls {
    padding-top: 0.5rem;
    padding-right: 0.93rem;
    padding-bottom: 0.5rem;
    padding-left: 0;
}

.apkls .apkls-img {
    position: relative;
    width: 3.5rem;
    height: 3.5rem;
    overflow: hidden;
}

.apkls .apkmore {
    text-align: left
}

.apkls .apkmore>h1 {
    font-weight: bold;
    margin-bottom: 0.1rem
}

.apkls .apkmore>h3 {
    color: #666;
    margin-top: 0.1rem;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.apkls .apkmore span {
    color: #999;
    margin-right: 1rem
}

.apkls .download {
    white-space: nowrap;
    cursor: pointer;
    box-sizing: border-box;
    border-radius: 3.125rem;
    font-size: 0.8125rem;
    border: 1px solid #05b4ff;
    color: #05b4ff;
    position: relative;
    background-color: #fff;
    text-align: center;
    padding: 0
}

.swiper {
    background: #fff;
    box-sizing: border-box;
    padding: 1rem 0 1rem 1rem;
}

.swiper.adimg {
